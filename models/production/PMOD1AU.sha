*NORMALIZE PRICES SO THAT ALL PRICES EQUAL ONE IN 1959
DO #=1,6
GENR C#=P#(1)
ENDO
DO #=1,6
GENR P#=P#/C#
GENR Y#=Y#*C#
ENDO 
